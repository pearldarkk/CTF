{
    "python.formatting.provider": "yapf",
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, IndentWidth: 4, ColumnLimit: 0 }",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "files.associations": {
        "*.c": "c",
        "*.cpp": "cpp",
        "*.py": "python",
        "*.java": "java",
        "*.jav": "java",
        "*.ja": "java",
        "ostream": "cpp",
        "iostream": "cpp",
        "stdio.h": "c",
        "wchar.h": "c",
        "*.tcc": "c",
        "bitset": "c",
        "compare": "c",
        "deque": "c",
        "string": "c",
        "vector": "c",
        "memory_resource": "c",
        "optional": "c",
        "functional": "c",
        "memory": "c",
        "istream": "c",
        "numeric": "c",
        "ranges": "c",
        "string_view": "c",
        "tuple": "c",
        "type_traits": "c",
        "typeinfo": "c"
    },
    "python.pythonPath": "/usr/bin/python3",
    "diffEditor.wordWrap": "off"
}